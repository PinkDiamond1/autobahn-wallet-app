{
  "expo": {
    "name": "Autobahn",
    "slug": "autobahn-wallet-app",
    "platforms": ["ios", "android", "web"],
    "scheme": "autobahn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app-icon.png",
    "splash": {
      "image": "./src/assets/app-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FA004D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
